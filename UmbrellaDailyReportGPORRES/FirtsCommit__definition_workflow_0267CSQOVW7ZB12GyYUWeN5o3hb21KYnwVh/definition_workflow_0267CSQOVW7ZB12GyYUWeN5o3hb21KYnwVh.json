        "type": "python3.script",
        "base_type": "activity",
        "properties": {
          "action_timeout": 180,
          "continue_on_failure": false,
          "display_name": "Details blocked",
          "script": "import sys\nimport json\nimport requests\n\nidentity2 = sys.argv[2]\nidentity1 = sys.argv[1]\nidentity3 = sys.argv[3]\nidentity4 = sys.argv[5]\nidentity5 = sys.argv[6]\n\nheaders = {\"Authorization\":f\"Bearer {sys.argv[4]}\"}\n\nactivity_dns = json.loads(sys.argv[7])['data']\nmalware_urls = []\nphishing_urls = []\ncommand_control_urls = []\nmisp_voseda_urls = []\nblock_list_urls = []\n\ni_urls_1 = []\ni_urls_2 = []\ni_urls_3 = []\ni_urls_4 = []\ni_urls_5 = []\ni_policy_1 = []\ni_policy_2 = []\ni_policy_3 = []\ni_policy_4 = []\ni_policy_5 = []\ni_ips_1 = []\ni_ips_2 = []\ni_ips_3 = []\ni_ips_4 = []\ni_ips_5 = []\n\n\n\n\nfor activity in activity_dns:\n    for category in activity['policycategories']:\n        if category['id'] == 67: malware_urls.append(activity['domain'])\n        if category['id'] == 68: phishing_urls.append(activity['domain'])\n        if category['id'] == 86: misp_voseda_urls.append(activity['domain'])\n        if category['id'] == 71: block_list_urls.append(activity['domain'])\n    for identity in activity['identities']:\n        if identity['label'] == identity1: \n            i_urls_1.append(activity['domain'])\n            i_policy_1.append(identity['type']['label'])\n            i_ips_1.append(activity['internalip']+\"/\"+activity['externalip'])\n        if identity['label'] == identity2: \n            i_urls_2.append(activity['domain'])\n            i_policy_2.append(identity['type']['label'])\n            i_ips_2.append(activity['internalip']+\"/\"+activity['externalip'])\n        if identity['label'] == identity3: \n            i_urls_3.append(activity['domain'])\n            i_policy_3.append(identity['type']['label'])\n            i_ips_3.append(activity['internalip']+\"/\"+activity['externalip'])\n        if identity['label'] == identity4: \n            i_urls_4.append(activity['domain'])\n            i_policy_4.append(identity['type']['label'])\n            i_ips_4.append(activity['internalip']+\"/\"+activity['externalip'])\n        if identity['label'] == identity5: \n            i_urls_5.append(activity['domain'])\n            i_policy_5.append(identity['type']['label'])\n            i_ips_5.append(activity['internalip']+\"/\"+activity['externalip'])\n\nmalware_urls = list(set(malware_urls))\nmalware_urls = ', '.join(malware_urls)\nphishing_urls = list(set(phishing_urls))\nphishing_urls = ', '.join(phishing_urls)\nmisp_voseda_urls = list(set(misp_voseda_urls))\nmisp_voseda_urls = ', '.join(misp_voseda_urls)\ncommand_control_urls = list(set(command_control_urls))\ncommand_control_urls = ', '.join(command_control_urls)\nblock_list_urls = list(set(block_list_urls))\nblock_list_urls = ', '.join(block_list_urls)\n\ni_urls_1 = list(set(i_urls_1))\ni_urls_1 = ', '.join(i_urls_1)\ni_urls_2 = list(set(i_urls_2))\ni_urls_2 = ', '.join(i_urls_2)\ni_urls_3 = list(set(i_urls_3))\ni_urls_3 = ', '.join(i_urls_3)\ni_urls_4 = list(set(i_urls_4))\ni_urls_4 = ', '.join(i_urls_4)\ni_urls_5 = list(set(i_urls_5))\ni_urls_5 = ', '.join(i_urls_5)\n\ni_policy_1 = list(set(i_policy_1))\ni_policy_1 = ', '.join(i_policy_1)\ni_policy_2 = list(set(i_policy_2))\ni_policy_2 = ', '.join(i_policy_2)\ni_policy_3 = list(set(i_policy_3))\ni_policy_3 = ', '.join(i_policy_3)\ni_policy_4 = list(set(i_policy_4))\ni_policy_4 = ', '.join(i_policy_4)\ni_policy_5 = list(set(i_policy_5))\ni_policy_5 = ', '.join(i_policy_5)\n\ni_ips_1 = list(set(i_ips_1))\nauxi = \"\"\nauxe = \"\"\nfor ip in i_ips_1:\n    auxi += ip.split(\"/\")[0]+\", \"\n    auxe += ip.split(\"/\")[1]+\", \"\ni_ips_1 = '\u003ctd\u003e'+auxi+\"\u003c/td\u003e\"+\"\u003ctd\u003e\"+auxe+\"\u003c/td\u003e\"\ni_ips_2 = list(set(i_ips_2))\nauxi = \"\"\nauxe = \"\"\nfor ip in i_ips_2:\n    auxi += ip.split(\"/\")[0]+\", \"\n    auxe += ip.split(\"/\")[1]+\", \"\ni_ips_2 = '\u003ctd\u003e'+auxi+\"\u003c/td\u003e\"+\"\u003ctd\u003e\"+auxe+\"\u003c/td\u003e\"\ni_ips_3 = list(set(i_ips_3))\nauxi = \"\"\nauxe = \"\"\nfor ip in i_ips_3:\n    auxi += ip.split(\"/