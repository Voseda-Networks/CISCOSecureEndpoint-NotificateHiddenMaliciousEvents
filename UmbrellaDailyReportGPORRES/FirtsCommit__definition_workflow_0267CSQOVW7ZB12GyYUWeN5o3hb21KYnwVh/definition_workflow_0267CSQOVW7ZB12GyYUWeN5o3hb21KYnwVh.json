nd i == 5:\n       category5=allowed_category['category']['label']\n       counts5=allowed_category['count'] if int(allowed_category['count'])\u003c1000 else str(\"{:.2f}\".format(allowed_category['count']/1000))+' k'\n       i=i+1\n       continue\n    \n\nfor blocked_category in blocked_categories:\n    if blocked_category['category']['label'] == 'Malware':\n        malware = blocked_category['count']\n    if blocked_category['category']['label'] == 'Phishing':\n        phishing = blocked_category['count']\n    if blocked_category['category']['label'] == 'command\u0026control':\n        command_control = blocked_category['count']\n    if blocked_category['category']['label'] == 'MISP Voseda':\n        misp_voseda = blocked_category['count']\n    if blocked_category['category']['label'] == 'Block List':\n        block_list = blocked_category['count']\nj=1\nfor blocked_identity in blocked_identities:\n    if j == 1:\n       identity1=blocked_identity['identity']['label']\n       i_counts1=blocked_identity['counts']['blockedrequests'] if int(blocked_identity['counts']['blockedrequests'])\u003c1000 else str(\"{:.2f}\".format(blocked_identity['counts']['blockedrequests']/1000))+' k'\n       j+=1\n       continue\n    if j == 2:\n       identity2=blocked_identity['identity']['label']\n       i_counts2=blocked_identity['counts']['blockedrequests'] if int(blocked_identity['counts']['blockedrequests'])\u003c1000 else str(\"{:.2f}\".format(blocked_identity['counts']['blockedrequests']/1000))+' k'\n       j+=1\n       continue\n    if j == 3:\n       identity3=blocked_identity['identity']['label']\n       i_counts3=blocked_identity['counts']['blockedrequests'] if int(blocked_identity['counts']['blockedrequests'])\u003c1000 else str(\"{:.2f}\".format(blocked_identity['counts']['blockedrequests']/1000))+' k'    \n       j+=1\n       continue\n    if j == 4:\n       identity4=blocked_identity['identity']['label']\n       i_counts4=blocked_identity['counts']['blockedrequests'] i